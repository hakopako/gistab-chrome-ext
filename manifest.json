{
  "name": "Gistab",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "Switch gist pages to tab style.",
  "homepage_url": "https://web.hakopako.net/gistab-chrome-ext/",
  "content_scripts": [
    {
      "matches": ["https://gist.github.com/*"],
      "js": ["gistab-min.js"],
      "run_at": "document_idle"
    }
  ]
}
