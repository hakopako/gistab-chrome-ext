{
  "name": "Gistab",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "file list to tabs for gist page.",
  "homepage_url": "https://github.com/hakopako/gistab-chrome-ext",
  "content_scripts": [
    {
      "matches": ["https://gist.github.com/*"],
      "js": ["jquery-2.1.4.min.js", "inject.js"],
      "run_at": "document_idle"
    }
  ]
}